head
    link(rel="stylesheet", href="styles/css/example.css")
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")
    script(src="js/example.js")
    
body
    .main-div
        h1 Pug File
        .input-container
            span Nombre 
            input#nombre(type="text")
        .send-button-container
            button.send Enviar
        .names-container
            ul 
                each name, index in names
                    li#target !{name}
                    .send-button-container
                        button(onclick=`eliminar(${JSON.stringify({name})})`) Delete
                    
               
